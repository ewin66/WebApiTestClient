<?xml version="1.0"?>
<package >
  <metadata>
    <id>matthewcv.WebApiTestClient</id>
    <version>1.0.9</version>
    <title>Web API Test Client</title>
    <authors>matthewcv</authors>
    <owners>matthewcv</owners>
    <projectUrl>https://github.com/matthewcv/WebApiTestClient</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Automatically generate a UI to test your ASP.Net Web API.  The UI is based on the description of your API and the ressources it exposes.  You get neat input elements instead of having to manually type all that JSON or XML.</description>
    <releaseNotes>see changelog.text</releaseNotes>
    <copyright>Copyright 2015</copyright>
    <tags>WebAPI Test</tags>
    <dependencies>
      <dependency id="Microsoft.AspNet.WebApi.WebHost" version="5.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\WebApiTestClient.Web\Content\WebApiTestClient.styles.css" target="Content\Content" />
    <file src="..\WebApiTestClient.Web\Content\WebApiTestClient.views.html" target="Content\Content" />
    <file src="..\WebApiTestClient.Web\Scripts\WebApiTestClient.js" target="Content\Scripts" />
  </files>
</package>